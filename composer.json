{
  "name": "drupal/recommended-project",
  "description": "Project template for Drupal 9 projects with a relocated document root",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "homepage": "https://www.drupal.org/project/drupal",
  "support": {
    "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
    "chat": "https://www.drupal.org/node/314178"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "require": {
    "composer/installers": "^1.9",
    "cweagans/composer-patches": "^1.7",
    "drupal/admin_toolbar": "^3.0",
    "drupal/bartik": "^1.0",
    "drupal/content_browser": "^1.0@alpha",
    "drupal/context": "^5",
    "drupal/controlled_access_terms": "^2",
    "drupal/core-composer-scaffold": "^10.2",
    "drupal/core-dev": "^10.2",
    "drupal/core-project-message": "^10.2",
    "drupal/core-recommended": "^10.2",
    "drupal/devel": "^5",
    "drupal/facets": "^2",
    "drupal/field_group": "^3.6",
    "drupal/field_permissions": "^1.4",
    "drupal/file_delete": "^2.0",
    "drupal/flood_control": "^3.0",
    "drupal/imce": "^3",
    "drupal/islandora": "^2",
    "drupal/jquery_ui_button": "^2",
    "drupal/jquery_ui_checkboxradio": "^2",
    "drupal/jquery_ui_slider": "^2",
    "drupal/matomo": "^1.7",
    "drupal/migrate_tools": "^6.0",
    "drupal/openseadragon": "^2",
    "drupal/pagerer": "^3",
    "drupal/pagerer_jqueryui": "^1.0@dev",
    "drupal/pdf": "1.x-dev",
    "drupal/queue_ui": "^3.2",
    "drupal/range": "^1.3",
    "drupal/rest_oai_pmh": "^2.0@beta",
    "drupal/restui": "^1.16",
    "drupal/search_api": "^1.28",
    "drupal/search_api_solr": "^4.1",
    "drupal/security_review": "^3.1",
    "drupal/simple_block": "^1.7",
    "drupal/token_filter": "^2",
    "drupal/token_views_filter": "^2",
    "drupal/twig_tweak": "^3.2",
    "drupal/upgrade_status": "^4.3",
    "drupal/views_bulk_edit": "^3",
    "drupal/views_bulk_operations": "^4.0",
    "drupal/views_data_export": "^1.3",
    "drupal/views_field_view": "^1.0@beta",
    "drush/drush": "^12",
    "easyrdf/easyrdf": "^1.1",
    "islandora-rdm/islandora_fits": "dev-8.x-1.x",
    "mjordan/islandora_workbench_integration": "dev-main",
    "zaporylie/composer-drupal-optimizations": "^1.1"
  },
  "conflict": {
    "drupal/drupal": "*",
    "drupal/flysystem": "<2.2",
    "drupal/rdf": ">=3"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "drupal/core-composer-scaffold": true,
      "drupal/core-project-message": true,
      "php-http/discovery": true,
      "phpstan/extension-installer": true,
      "tbachert/spi": true,
      "zaporylie/composer-drupal-optimizations": true
    }
  },
  "extra": {
    "patches": {
      "drupal/core": {
        "Disable rollback migration operation": "./patches/form.css.patch"
      }
    },
    "drupal-scaffold": {
      "locations": {
        "web-root": "web/"
      },
      "file-mapping": {
        "[web-root]/robots.txt": {
          "append": "assets/robots-additions.txt"
        }
      }
    },
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/Commands/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "web/modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "web/profiles/custom/{$name}": [
        "type:drupal-custom-profile"
      ],
      "web/themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ]
    },
    "drupal-core-project-message": {
      "include-keys": [
        "homepage",
        "support"
      ],
      "post-create-project-cmd-message": [
        "<bg=blue;fg=white>                                                         </>",
        "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
        "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
        "<bg=blue;fg=white>                                                         </>",
        "",
        "<bg=yellow;fg=black>Next steps</>:",
        "  * Install the site: https://www.drupal.org/docs/8/install",
        "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
        "  * Get support: https://www.drupal.org/support",
        "  * Get involved with the Drupal community:",
        "      https://www.drupal.org/getting-involved",
        "  * Remove the plugin that prints this message:",
        "      composer remove drupal/core-project-message"
      ]
    }
  }
}
